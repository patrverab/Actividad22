<h1>Students</h1>


<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope= "col"> Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.name %></td>
        <td><%= student.email %></td>

      </tr>
    <% end %>
  </tbody>
</table>
